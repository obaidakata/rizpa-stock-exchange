<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<ScrollPane prefHeight="815.0" prefWidth="1125.0" xmlns="http://javafx.com/javafx/8.0.171"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.RizpaController">
    <content>
        <BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
                    prefHeight="812.0" prefWidth="1121.0">
            <center>
                <TabPane fx:id="usersPanel" prefHeight="1200.0" prefWidth="1080.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                        <Tab text="Admin">
                            <content>
                                <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="715.0" prefWidth="1072.0">
                                    <children>
                                        <fx:include fx:id="adminPage" source="AdminFX.fxml"/>
                                    </children>
                                </AnchorPane>
                            </content>
                        </Tab>
                    </tabs>
                </TabPane>
            </center>
            <top>
                <GridPane BorderPane.alignment="CENTER">
                    <columnConstraints>
                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0"/>
                    </columnConstraints>
                    <rowConstraints>
                        <RowConstraints minHeight="10.0" prefHeight="40.0" vgrow="SOMETIMES"/>
                    </rowConstraints>
                    <children>
                        <Label alignment="CENTER" text="Rizpa Stock Exchange" textAlignment="CENTER"
                               GridPane.columnIndex="2"/>
                        <Button fx:id="loadFileButton" mnemonicParsing="false" onAction="#loadFile" text="Load File"/>
                        <Label fx:id="fileStatus" prefHeight="17.0" prefWidth="175.0" GridPane.columnIndex="1"/>
                        <Label text="System Messages" GridPane.columnIndex="4"/>
                    </children>
                </GridPane>
            </top>
            <right>
                <ListView fx:id="messagesList" prefHeight="772.0" prefWidth="271.0" BorderPane.alignment="CENTER"/>
            </right>
        </BorderPane>
    </content>
</ScrollPane>
